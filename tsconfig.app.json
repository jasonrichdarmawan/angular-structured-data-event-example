/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "paths": {
      "@common/pipes": [
        "dist/common/pipes"
      ],
      "@common/pipes/*": [
        "dist/common/pipes/*"
      ],
      "@features/experience": [
        "dist/features/experience"
      ],
      "@features/experience/*": [
        "dist/features/experience/*"
      ]
    },
    "outDir": "./out-tsc/app",
    "types": []
  },
  "files": [
    "src/main.ts",
    "src/polyfills.ts"
  ],
  "include": [
    "src/**/*.d.ts"
  ]
}
